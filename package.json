{
  "name": "my-portfolio",
  "version": "0.1.0",
  "description": "My portfolio with React using Typescript.",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "roottool",
    "url": "https://github.com/roottool"
  },
  "homepage": "https://roottool.github.io/",
  "repository": "https://github.com/roottool/my-portfolio.git",
  "bugs": {
    "url": "https://github.com/roottool/my-portfolio/issues"
  },
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "prepare": "npx is-ci || husky install",
    "start": "react-scripts start",
    "build": "run-p build:*",
    "build:react": "react-scripts build",
    "lint": "run-p -c lint:*",
    "lint:check-types": "tsc --noEmit",
    "lint:eslint": "eslint . --color --ext .js,.jsx,.ts,.tsx",
    "lint:prettier": "prettier --check .",
    "fix": "run-p -c fix:*",
    "fix:eslint": "eslint . --fix --color --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write .",
    "test": "run-p test:*",
    "test:jest": "echo \"Write tests!\"",
    "test:check-types": "tsc --noEmit"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "lint-staged": {
    "*.{js|jsx|ts|tsx}": [
      "yarn fix",
      "yarn format"
    ],
    "*.{json|md}": [
      "yarn format"
    ],
    ".{eslint|prettier}rc": [
      "yarn format"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.5.1",
    "@types/jest": "26.0.22",
    "@types/node": "14.14.41",
    "@types/react": "^17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/react-helmet": "^6.1.1",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.6",
    "@types/styled-components": "^5.1.9",
    "axios": "^0.21.2",
    "connected-react-router": "^6.9.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.2.0",
    "react-redux": "^7.2.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "4.0.3",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.2.3",
    "typescript": "4.2.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.5",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "eslint-plugin-typescript-sort-keys": "^1.6.0",
    "husky": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "lint-staged": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "redux-devtools-extension": "^2.13.9"
  }
}
